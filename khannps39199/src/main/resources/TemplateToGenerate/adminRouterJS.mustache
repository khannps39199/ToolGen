
const listDashBoard = [
{{#fields}}
    "{{className}}",
{{/fields}}
]
export default [
{{#fields}}  
	{
        path: '{{className}}',
        component: import('../../components/Admin/{{className}}/index.vue'),
        props: route => ({ TableName: '{{className}}'})
    },
    {
        path: '{{className}}/create',
        component:  () => import('../../components/Admin/{{className}}/Form.vue'),
        props: route => ({ TableName: '{{className}}', action: 'create'})
    },
    {
        path: '{{className}}/view/:id',
        component:  () => import('../../components/Admin/{{className}}/Form.vue'),
        props: route => ({ TableName: '{{className}}', action: 'view', id: route.params.id,})
    },
    {
        path: '{{className}}/update/:id',
        component:  () => import('../../components/Admin/{{className}}/Form.vue'),
        props: route => ({ TableName: '{{className}}', action: 'update', id: route.params.id,})
    },
{{/fields}}
]
